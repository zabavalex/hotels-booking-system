  "scripts": {
    "start": "craco start",
    "dev": "npm run start --",
    "dev:fake": "npm run start -- --fake",
    "build": "cross-env GENERATE_SOURCEMAP=false TSC_COMPILE_ON_ERROR=true craco build",
    "test": "craco test",
    "cosmos": "cross-env FAST_REFRESH=false cosmos",
    "check-types": "tsc",
    "lint": "eslint src/**/*.{js,ts,tsx}",
    "stylelint": "stylelint src/**/*.scss",
    "lint:fix": "npm run lint -- --fix",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,css,scss,md}\"",
    "analyze:prepare": "cross-env TSC_COMPILE_ON_ERROR=true react-app-rewired build",
    "analyze:main": "source-map-explorer 'build/static/js/*.js'",
    "analyze": "run-s analyze:prepare analyze:main",
    "prepare-config": "node scripts/prepare-config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
